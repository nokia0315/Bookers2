
<%= render 'users/list', user: @user, book:@book %>


<% @books.each do |book| %>
<div>

  <%= link_to book_path(book.id) do %>
  <p><%= attachment_image_tag book.user, :profile_image, :fill, 60, 60, fallback: "no_image.jpg" %></p>
  <p>Title：<%= book.title %></p>
  <p>Opinion：<%= book.body %></p>
  <% end %>
</div>
<% end %>